{
    "compilerOptions": {
      "target": "ES2022",
      "module": "nodeNext",
      "moduleResolution": "nodeNext",
      "outDir": "./dist",
      "rootDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "resolveJsonModule": true
    },
    "include": [
      "*.ts",
      "lib/**/*.ts"
, "test/test.ts"    ],
    "exclude": [
      "node_modules"
    ]
  }